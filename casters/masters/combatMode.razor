if varexist debug
    sysmsg 'caster.masters.combatMode.razor.razor'
    overhead 'caster.masters.combatMode.razor.razor'
endif
// Set & Unset PK Mode
// set variable PkMode to true or false for use in other scripts
if varexist PKmode
    unsetvar! Pkmode
    overhead "Monster Mode"
    warmode off
else
    setvar! PKmode 1
    overhead "Player Mode"
    warmode on
endif
