if varexist debug
    sysmsg 'Mage.life_support.areParalized.razor'
    overhead 'Mage.life_support.areParalized.razor'
endif
while paralyzed and varexist havePopPouch
    if findtype "pouch" backpack 38 
            yell "[pouch"
            wait 200
    else
        overhead "Out of TrapPouches!" 34
        @unsetvar havePopPouch
        script 'Mage\masters\lifeSupport'
    endif
endwhile
script 'Mage\masters\lifeSupport'
