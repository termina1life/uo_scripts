if not bandaging and varexist superBandages
    if findtype "clean bandage%s%" backpack
        hotkey "Bandage Self"
        wait 100
    else 
        overhead "Out of bandages!" 34
        @unsetvar haveBandages
    wait 100
endwhile

if poisoned
    while poisoned
        if findtype "Orange Potion" backpack
            hotkey 'Drink Cure'
            wait 300
        elseif poisoned and not findtype "Orange Potion" backpack
            @unsetvar haveCurePots
            break
        endif
    endwhile
endif
if poisoned and varexist haveMagery
    if varexist superBandages and not bandaging and diffhits > 30
        script 'harvesting\mining\life_support\magery'
    else
        script 'harvesting\mining\life_support\lifeSupport'
    endif
endif
if poisoned and varexist haveChivalry
    if varexist superBandages and not bandagingand diffhits > 30
        script 'harvesting\mining\life_support\chivalry'
    else
        script 'harvesting\mining\life_support\lifeSupport'
    endif
endif
script 'harvesting\mining\life_support\lifeSupport'
        