    if hp < maxhp
        if diffhits < 3 and poisoned
            while not dead and not targetexists
                cast "Cure"
                wait 100
            endwhile
            hotkey "Target Self"
            replay
        elseif diffhits <= 11
            while not dead and not targetexists
                cast "Heal"
            wait 150
            endwhile
            hotkey "Target Self"
            wait 200
        elseif diffhits >= 20
            while not dead and not targetexists
                if diffhits >= 100 and diffhits <= 80
                    cast "Heal"
                else
                    cast "Greater Heal"    
                endif
                wait 200
            endwhile
            hotkey "Target Self"
            wait 100
            replay
        elseif poisoned
            while not dead and  not targetexists
                cast "Cure"
                wait 100
            endwhile
            hotkey "Target Self"
        endif
        wait 100
        replay
    endif
