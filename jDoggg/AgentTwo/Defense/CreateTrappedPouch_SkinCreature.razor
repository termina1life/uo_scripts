if paralyzed
    hotkey 'Play Script: AgentTwo\Defense\TriggerTrappedPouch'
endif
if varexist setPKmode and setPKmode = 1
    if findtype 'pouch' backpack 0
        cast 'Magic Trap'
        waitfortarget
        target 'self'
    else
        overhead 'pouches all trapped'
    endif
elseif varexist setPKmode and setPKmode = 0
    if findtype 'Elven Spellblade' as x
        dclick x
        waitfortarget
        target 'self'
    else
        overhead 'No skinning knife'
    endif
else
    overhead 'CreateTrappedPouch_SkinCreature broken'
endif